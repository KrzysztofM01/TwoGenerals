<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name = "database.CardSuggest" table = "tcardSuggest">

        <meta attribute = "class-description">
            Card Suggestion mapping
        </meta>

        <id name = "id" type = "int" column = "id">
            <generator class="native"/>
        </id>

        <property name = "name" column = "name" type = "string"/>
        <property name = "power" column = "power" type = "int"/>
        <property name = "cost" column = "cost" type = "int"/>
        <property name = "cardType" column = "cardType" type = "string"/>
        <property name = "imageURL" column = "imageURL" type = "string"/>

    </class>
</hibernate-mapping>